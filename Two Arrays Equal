class Solution:
    def checkEqual(self, a, b) -> bool:
        #code here
        if len(a) != len(b):
            return False
        freq = {}
        for num in a:
            if num in freq:
                freq[num] += 1
            else:
                freq[num] = 1
        for num in b:
            if num not in freq:
                return False
            else:
                freq[num] -= 1
            if freq[num] == 0:
                del(freq[num])
        return True
