class Solution:
    def canConstruct(self, ransomNote: str, magazine: str) -> bool:
        myDict = dict()
        for ch in magazine:
            if ch not in myDict:
                myDict[ch] = 1
            else:
                myDict[ch] += 1
        for ch in ransomNote:
            if ch not in myDict:
                return False
            else:
                myDict[ch] -= 1
                if myDict[ch] == 0:
                    del(myDict[ch])
        return True
