class Solution:
    def findDuplicates(self, nums: List[int]) -> List[int]:
        freq = {}
        result = []
        for n in nums:
            if n not in freq:
                freq[n] = 1
            else:
                freq[n] += 1
        for key, value in freq.items():
            if value == 2:
                result.append(key)
        return result
