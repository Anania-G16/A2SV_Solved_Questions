class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        result = ""
        stop = False
        for i in range (len(strs[0])):
            letter = strs[0][i] #f
            for word in strs:
                if i == len(word):
                    return result
                if word[i] != letter:
                    return result
            result += letter
        return result
        
