class Solution:
    def findRestaurant(self, list1: List[str], list2: List[str]) -> List[str]:
        answer = []
        indexSum = 0
        minIndexSum = 2001
        for word in list1:
            if word in list2:
                indexSum = list1.index(word) + list2.index(word)
                if indexSum < minIndexSum:
                    minIndexSum = indexSum
                    answer.clear()
                    answer.append(word)
                elif indexSum == minIndexSum:
                    answer.append(word)
        return answer
