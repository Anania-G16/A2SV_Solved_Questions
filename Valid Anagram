class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        myDict = dict()
        for char in s:
            if char not in myDict:
                myDict[char] = 1
            else:
                myDict[char] += 1
        for char in t:
            if char not in myDict:
                return False
            else:
                myDict[char] -= 1
                if myDict[char] == 0:
                    del(myDict[char])
        if len(myDict) != 0:
            return False
        return True
        
