class Solution:
    #Function to check if a is a subset of b.
    def isSubset(self, a, b):
        if len(b) > len (a):
            return False
        freq = {}
        for num in a:
            if num in freq:
                freq[num] += 1
            else:
                freq[num] = 1
        for num in b:
            if num in freq:
                freq[num] -= 1
            else:
                return False
            if freq[num] == 0:
                del(freq[num])
        return True
    
